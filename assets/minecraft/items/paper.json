{
    "model": {
        "type": "select",
        "property": "custom_model_data",
        "fallback": {
            "type": "model",
            "model": "minecraft:item/paper"
        },
        "cases": [
            {
                "when": "iron",
                "model": {
                    "type": "model",
                    "model": "permits:item/iron_permit"
                }
            },
            {
                "when": "gold",
                "model": {
                    "type": "model",
                    "model": "permits:item/gold_permit"
                }
            },
            {
                "when": "diamond",
                "model": {
                    "type": "model",
                    "model": "permits:item/diamond_permit"
                }
            }
        ]
    }
}